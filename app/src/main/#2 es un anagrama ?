#2 es un anagrama?
#doko

def anagrama(p1,p2):
  if p1 == p2:
    return False
  elif p1 != p2:
    if len(p1) == len(p2):
      dict1 = {}
      dict2 = {}
      
      for x in p1:
        if x in dict1: 
          dict1[x] += 1
        else: dict1[x] = 1
      
      for i in p2:
        if i in dict2: 
          dict2[i] += 1
        else: dict2[i] = 1
      return dict1 == dict2
    return False

a = "amor"
b = "roma"
asd = print(anagrama(a,b))
